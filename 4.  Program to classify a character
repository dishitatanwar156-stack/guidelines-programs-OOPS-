# Program to classify a character

ch = input("Enter a character: ")

if len(ch) != 1:
    print("Please enter only one character.")
else:
    if ch.isalpha():   # check if it is a letter
        print(f"{ch} is a letter.")
        if ch.isupper():
            print("It is an uppercase letter.")
        else:
            print("It is a lowercase letter.")
    elif ch.isdigit():  # check if it is a digit
        print(f"{ch} is a numeric digit.")
    else:
        print(f"{ch} is a special character.")



        def digit_to_text(d):
    names = {
        '0': "ZERO",
        '1': "ONE",
        '2': "TWO",
        '3': "THREE",
        '4': "FOUR",
        '5': "FIVE",
        '6': "SIX",
        '7': "SEVEN",
        '8': "EIGHT",
        '9': "NINE"
    }
    return names.get(d, "Not a digit")

# Main program
ch = input("Enter a character: ")

if len(ch) != 1:
    print("Please enter only one character.")
elif ch.isdigit():
    print(f"In words: {digit_to_text(ch)}")
else:
    print("The input is not a numeric digit.")
