# Program to create a list of cubes of even integers

# Input list can have mixed data types
input_list = [1, 2, "hello", 4, 5.5, 6, "abc", 7, 8]

cubes = []  # result list

for item in input_list:
    if isinstance(item, int) and item % 2 == 0:  # check even integer
        cubes.append(item ** 3)

print("Input List:", input_list)
print("Cubes of even integers:", cubes)







# Input list can have mixed data types
input_list = [1, 2, "hello", 4, 5.5, 6, "abc", 7, 8]

# List comprehension for cubes of even integers
cubes = [x**3 for x in input_list if isinstance(x, int) and x % 2 == 0]

print("Input List:", input_list)
print("Cubes of even integers:", cubes)


